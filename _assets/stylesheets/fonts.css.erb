@font-face {
  font-family: 'Blanch Caps';
  src: url('<%= asset_path "BLANCH_CAPS_LIGHT.eot" %>'); /* IE9 Compat Modes */
  src: url('<%= asset_path "BLANCH_CAPS_LIGHT.eot" %>?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('<%= asset_path "BLANCH_CAPS_LIGHT.woff" %>') format('woff'), /* Modern Browsers */
       url('<%= asset_path "BLANCH_CAPS_LIGHT.ttf" %>')  format('truetype'), /* Safari, Android, iOS */
       url('<%= asset_path "BLANCH_CAPS_LIGHT.svg" %>#0360c398b36e1cadc260eace665fcba3') format('svg'); /* Legacy iOS */

  font-style:   normal;
  font-weight:  200;
}
